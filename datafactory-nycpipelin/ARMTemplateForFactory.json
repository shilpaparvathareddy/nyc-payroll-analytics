{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-nycpipelin"
		},
		"ls_adls_nycpayroll_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_nycpayroll'"
		},
		"ls_sqldb_nycpayroll_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sqldb_nycpayroll'"
		},
		"ls_adls_nycpayroll_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayrollshi.dfs.core.windows.net/"
		},
		"ls_sqldb_nycpayroll_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "projectnycpayro.database.windows.net"
		},
		"ls_sqldb_nycpayroll_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-nycpayroll"
		},
		"ls_sqldb_nycpayroll_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqldbadmin"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ls_adls_nycpayroll')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('ls_adls_nycpayroll_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('ls_adls_nycpayroll_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_sqldb_nycpayroll')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"server": "[parameters('ls_sqldb_nycpayroll_properties_typeProperties_server')]",
					"database": "[parameters('ls_sqldb_nycpayroll_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('ls_sqldb_nycpayroll_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('ls_sqldb_nycpayroll_password')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}